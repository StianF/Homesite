<%= form_for @employment do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :company %><br />
    <%= f.text_field :company %>
  </p>
  <p>
    <%= f.label :from %><br />
    <%= f.datetime_select :from %>
  </p>
  <p>
    <%= f.label :to %><br />
    <%= f.datetime_select :to %>
  </p>
  <p>
    <%= f.label :current %><br />
    <%= f.check_box :current %>
  </p>
  <p>
    <%= f.label :title_no %><br />
    <%= f.text_field :title_no %>
  </p>
  <p>
    <%= f.label :title_en %><br />
    <%= f.text_field :title_en %>
  </p>
  <p>
    <%= f.label :description_no %><br />
    <%= f.text_area :description_no %>
  </p>
  <p>
    <%= f.label :description_en %><br />
    <%= f.text_area :description_en %>
  </p>

  <p><%= f.submit %></p>
<% end %>
